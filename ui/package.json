{
  "name": "quasar-ui-easy-tables",
  "version": "0.5.3",
  "author": "Luca Ban - Mesqueeb",
  "description": "A Vue plugin (that uses Quasar components) to easily generate tables by only defining a \"schema\" object.",
  "license": "MIT",
  "module": "dist/index.esm.js",
  "main": "dist/index.common.js",
  "scripts": {
    "deploy": "npm run build && cd dev && npm run deploy && cd .. && npm publish && cd ../app-extension && npm publish",
    "dev": "cd dev && yarn dev && cd ..",
    "dev:umd": "yarn build && node build/script.open-umd.js",
    "dev:ssr": "cd dev && yarn 'dev:ssr' && cd ..",
    "dev:ios": "cd dev && yarn 'dev:ios' && cd ..",
    "dev:android": "cd dev && yarn 'dev:android' && cd ..",
    "dev:electron": "cd dev && yarn 'dev:electron' && cd ..",
    "build": "node build/index.js",
    "build:js": "node build/script.javascript.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mesqueeb/quasar-ui-easy-tables"
  },
  "bugs": "https://github.com/mesqueeb/quasar-ui-easy-tables/issues",
  "homepage": "https://quasar-easy-tables.web.app",
  "peerDependencies": {
    "quasar-ui-easy-forms": "^1.1.3"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.20.0",
    "@rollup/plugin-json": "^4.0.1",
    "chalk": "^2.4.2",
    "fs-extra": "^8.1.0",
    "open": "^6.4.0",
    "quasar": "^1.5.11",
    "quasar-ui-easy-forms": "^1.1.3",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.14",
    "rollup-plugin-vue": "^5.1.4",
    "uglify-es": "^3.3.9",
    "vue-template-compiler": "^2.6.11",
    "zlib": "^1.0.5"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ],
  "dependencies": {
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "commafy-anything": "^1.1.1",
    "copy-anything": "^1.5.1",
    "flatten-anything": "^1.4.1",
    "is-what": "^3.5.0",
    "merge-anything": "^2.4.4",
    "nestify-anything": "0.0.0",
    "path-to-prop": "0.0.1"
  }
}
